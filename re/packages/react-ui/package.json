{
  "name": "@re/react-ui",
  "version": "0.1.0",
  "description": "React UI components for Re Generative UI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/index.css",
    "./src/styles/index.css": "./src/styles/index.css"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react --external react-dom && node -e \"require('fs').copyFileSync('src/styles/index.css', 'dist/index.css')\"",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --external react-dom --watch"
  },
  "keywords": [
    "react",
    "ui",
    "components"
  ],
  "license": "MIT",
  "dependencies": {
    "@re/core": "*",
    "recharts": "^2.10.3",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}